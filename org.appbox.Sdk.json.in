{
    "build-runtime": true,
    "id": "org.appbox.Sdk",
    "id-platform": "org.appbox.Platform",
    "branch": "@@SDK_BRANCH@@",
    "runtime": "org.gnome.Platform",
    "sdk": "org.gnome.Sdk",
    "runtime-version": "@@SDK_RUNTIME_VERSION@@",
    "writable-sdk": true,
    "sdk-extensions": [ "org.gnome.Sdk.Debug", "org.gnome.Sdk.Locale"],
    "platform-extensions": [ "org.gnome.Platform.Locale"],
    "metadata": "metadata.sdk",
    "metadata-platform": "metadata.platform",
    "cleanup": [ "/cache",
                 "/man",
                 "/share/man",
                 "/share/gtk-doc",
                 "/share/devhelp",
                 "/lib/systemd",
                 "*.la", "*.a", "*.dll.mdb"],
    "cleanup-commands": [ "touch -d @0 /usr/share/fonts",
                          "touch -d @0 /usr/share/fonts/*",
                          "fc-cache -fs"
                        ],
    "cleanup-platform": [ "/include",
                          "/share/aclocal",
                          "/share/pkgconfig",
                          "/lib/pkgconfig",
                          "/share/gir-1.0",
                          "/share/vala"
                        ],
    "build-options" : {
        "cflags": "-O2 -g",
        "cxxflags": "-O2 -g",
        "env": {
            "V": "1"
        }
    },
    "modules": [
	{
            "name": "gdl",
            "sources": [
                {
                    "type": "archive",
                    "url": "https://download.gnome.org/sources/gdl/3.20/gdl-3.20.0.tar.xz",
                    "sha256": "53d3a3bb9b9be25b3a40c644fdbbb57a5a63ee1f5f839c2266d1cd9779360e8b"
                }
            ]
        },
	{
            "name": "webkitgtk3",
            "config-opts": [ "--with-gtk=3.0", "--disable-webkit2", "--disable-gtk-doc-html" ],
            "sources": [
                {
                    "type": "archive",
                    "url": "http://webkitgtk.org/releases/webkitgtk-2.4.11.tar.xz",
                    "sha256": "588aea051bfbacced27fdfe0335a957dca839ebe36aa548df39c7bbafdb65bf7"
                }
            ]
        },
        {
            "name": "mono-core",
            "config-opts": [ "--with-sgen=yes"],
            "sources": [
                {
                    "type": "archive",
                    "url": "http://download.mono-project.com/sources/mono/mono-4.4.0.182.tar.bz2",
                    "sha256": "983e27a054bc3dc6709b32b8de45a0f4335e9e9c3911b20d29f63eb7cc00bfe5"
                }
            ]
        },
        {
            "name": "gtk-sharp3",
            "sources": [
                {
                    "type": "archive",
                    "url": "https://github.com/openmedicus/gtk-sharp/archive/gtk-sharp-3.20.4.tar.gz",
                    "sha256": "8b52da8784a240f451414b5f8d6a13becd89a0be6cadf2eedbe395e6908dbb74"
                }
            ]
        },
	{
            "name": "mono-addins",
            "config-opts": [ "--disable-gui"],
            "sources": [
                {
                    "type": "archive",
                    "url": "https://github.com/mono/mono-addins/archive/mono-addins-1.2.tar.gz",
                    "sha256": "ca1fb765bbf21adf188b8b9407279bbcf29919f2b3afae7dd1e680737a257e62"
                },
		{
                    "type": "patch",
                    "path": "mono-addins-1.2-mcs.patch"
                }
            ]
        },
        {
            "name": "gdl-sharp",
            "sources": [
                {
                    "type": "archive",
                    "url": "https://github.com/openmedicus/gdl-sharp/archive/gdl-sharp-3.20.0.tar.gz",
                    "sha256": "ac6e17c2795d284c345d6437ba73206a1a65fcbeadfef8881f39e8b9ee2a3e8d"
                }
            ]
        },
	{
            "name": "dbus-sharp",
            "sources": [
                {
                    "type": "archive",
                    "url": "https://github.com/openmedicus/dbus-sharp/archive/dbus-sharp-0.9.0.tar.gz",
                    "sha256": "81bfa0a10b31d2e469bf7523b62c866a41c5a13bfad2e7889521abcaadf08116"
                }
            ]
        },
	{
            "name": "dbus-sharp-glib",
            "sources": [
                {
                    "type": "archive",
                    "url": "https://github.com/mono/dbus-sharp-glib/archive/v0.6.tar.gz",
                    "sha256": "d337d5a4dbaab2084972f85d175ba30ca87aa6baaa635288f3b37ca71f2d9047"
                },
		{
                    "type": "patch",
                    "path": "ndesk-dbus-glib-0.4.2.patch"
                },
		{
                    "type": "patch",
                    "path": "dbus-sharp-glib-0.6.0-mcs.patch"
                }
            ]
        },
	{
            "name": "webkitgtk3-sharp",
            "sources": [
                {
                    "type": "archive",
                    "url": "https://github.com/openmedicus/webkitgtk3-sharp/archive/webkitgtk3-sharp-2.4.9.tar.gz",
                    "sha256": "5b35f451aede62a7d5d4978371f56d7399c186be9edeefb73900ea154da999bc"
                }
            ]
        },
	{
            "name": "Newtonsoft.Json",
            "sources": [
                {
                    "type": "archive",
                    "url": "https://github.com/JamesNK/Newtonsoft.Json/archive/8.0.3.tar.gz",
                    "sha256": "f55ae3e7641ddda03ca3203819d432ecf7c353fb000c6ac6914f01a5393023df"
                },
		{
                    "type": "patch",
                    "path": "Newtonsoft.Json.patch"
                },
		{
                    "type": "file",
                    "path": "configure",
                    "dest-filename": "configure"
                }
            ]
        },
	{
            "name": "Npgsql",
            "sources": [
                {
                    "type": "archive",
                    "url": "https://github.com/npgsql/npgsql/archive/v3.1.3.tar.gz",
                    "sha256": "855686537e1c62b3391d6ce5352448fb712b786bab6d1c2b75e7a2d8e88cc0b3"
                },
		{
                    "type": "patch",
                    "path": "npgsql-confgire.patch"
                },
		{
                    "type": "file",
                    "path": "configure",
                    "dest-filename": "configure"
                }
            ]
        }
    ]
}
